{
  "name": "code-review-tool-backend",
  "version": "0.1.0",
  "private": true,
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon --config nodemon.json",
    "build": "tsc",
    "start": "node dist/index.js",
    "lint": "eslint . --ext .ts",
    "typecheck": "tsc --noEmit",
    "migration:run": "typeorm-ts-node-commonjs migration:run -d src/data-source.ts",
    "migration:revert": "typeorm-ts-node-commonjs migration:revert -d src/data-source.ts",
    "migration:create": "typeorm-ts-node-commonjs migration:create",
    "ngrok": "ngrok http 3001",
    "setup-ngrok": "node scripts/setup-ngrok.js",
    "fix-ngrok-config": "node scripts/fix-ngrok-config.js",
    "detect-ngrok": "node scripts/detect-ngrok.js",
    "dev:with-ngrok": "concurrently \"npm run dev\" \"npm run ngrok\""
  },
  "dependencies": {
    "@langchain/community": "^0.0.10",
    "@langchain/core": "^0.1.4",
    "@langchain/openai": "^0.0.12",
    "bcrypt": "^5.1.0",
    "chromadb": "^1.5.5",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.1",
    "langchain": "^0.1.0",
    "lodash": "^4.17.21",
    "morgan": "^1.10.0",
    "mysql2": "^3.5.2",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.3.17",
    "zod": "^3.21.4",
    "chroma-js": "^2.1.1",
    "glob": "^10.3.10"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/lodash": "^4.14.196",
    "@types/morgan": "^1.9.4",
    "@types/node": "^20.4.5",
    "@typescript-eslint/eslint-plugin": "^6.2.0",
    "@typescript-eslint/parser": "^6.2.0",
    "eslint": "^8.46.0",
    "nodemon": "^3.0.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6",
    "concurrently": "^8.2.0",
    "axios": "^1.6.0",
    "ngrok": "^5.0.0-beta.2"
  }
}
